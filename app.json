        "backgroundColor": "#ffffff"
      },
      "package": "com.geotag.camera",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCvAS2turpuQTaaPXfYf7oLVMxfm7G9S9w"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7f1f7592-abdd-4feb-96a9-1ab10ee69806"
      }
    }
  }
